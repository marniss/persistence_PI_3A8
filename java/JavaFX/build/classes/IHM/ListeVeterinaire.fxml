<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import org.controlsfx.control.Rating?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="692.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="IHM.ListeVeterinaireController">
    <stylesheets>
        <URL value="@listeveterinaire.css" />
    </stylesheets>
   <children>
      <Label layoutX="118.0" layoutY="29.0" prefHeight="39.0" prefWidth="344.0" text="La Liste Des Vétérinaires">
         <font>
            <Font name="System Bold Italic" size="27.0" />
         </font>
      </Label>
      <TableView fx:id="listevet" layoutX="28.0" layoutY="138.0" prefHeight="226.0" prefWidth="452.0">
         <columns>
            <TableColumn fx:id="nom" prefWidth="75.0" text="nom" />
            <TableColumn fx:id="prenom" prefWidth="75.0" text="prenom" />
            <TableColumn fx:id="tel" prefWidth="75.0" text="Tel" />
            <TableColumn fx:id="adresse" prefWidth="75.0" text="Adresse" />
            <TableColumn fx:id="mail" prefWidth="75.0" text="Mail" />
            <TableColumn fx:id="photo" prefWidth="75.0" text="Photo" />
         </columns>
      </TableView>
      <Label fx:id="nomvet" layoutX="524.0" layoutY="159.0" text="Label" />
      <Label fx:id="note" layoutX="524.0" layoutY="208.0" text="Label" />
      <Label layoutX="487.0" layoutY="251.0" text="affecter une note" />
      <TextField fx:id="lanote" layoutX="615.0" layoutY="247.0" prefHeight="25.0" prefWidth="67.0" />
      <Button fx:id="valide" layoutX="538.0" layoutY="312.0" mnemonicParsing="false" onAction="#valider" text="Valider" />
      <Rating fx:id="rateVet" layoutX="490.0" layoutY="112.0" onMouseClicked="#evalvet" partialRating="true" rating="0.0" />
   </children>
</AnchorPane>
