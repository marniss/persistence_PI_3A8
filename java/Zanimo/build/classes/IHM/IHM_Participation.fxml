<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="IHM.IHM_ParticipationController">
   <children>
      <Label layoutX="100.0" layoutY="21.0" text="En tant que membre je veux choisir mon animal que je vais participer avec dans le concours que j'ai choisi" />
      <Label fx:id="typeconcour" layoutX="63.0" layoutY="80.0" text="Label" />
      <TableView fx:id="tvListinAnimal" layoutX="227.0" layoutY="80.0" prefHeight="200.0" prefWidth="235.0">
        <columns>
          <TableColumn fx:id="id_anim" minWidth="2.0" prefWidth="38.0" text="ID" />
          <TableColumn fx:id="nom_anim" prefWidth="196.0" text="Nom" />
        </columns>
      </TableView>
      <Button fx:id="btnConfirmer" layoutX="324.0" layoutY="340.0" mnemonicParsing="false" onAction="#onClickConfirmer" text="Confirmer" />
      <Label fx:id="idMembre" layoutX="63.0" layoutY="113.0" text="Label" />
   </children>
</AnchorPane>
